.music-container{
  margin: 10px 0;
  width: 100%;
  height: 317px;
  background:rgba(255,255,255,1);
  box-shadow:0px 8px 22px 2px rgba(245,245,245,0.27);
  border-radius:10px;
}
.music-play-view{
  width: 100%;
  height: 86px;
  display: flex
}
.music-pic-view{
  margin-left: 23px;
  margin-top: 22px;
  width: 42px;
  height: 42px;
  background:rgba(191,191,191,1);
  border-radius:50%;
  overflow: hidden;
}
.music-singerImg{
  width: 42px;
  height: 42px;
}
.music-play-contentView{
  margin-left: 10px;
  margin-top: 27px;
  width: calc(100% - 125px);
  height: 37px;
}
.music-play-btn{
  margin-right: 23px;
  margin-top: 27px;
  width: 32px;
  height: 32px;
  background-color: #57BD6A;
  border-radius:50%;
  overflow: hidden;
}
.music-playBtnImg{
  width: 32px;
  height: 32px;
}
.music-songName{
  width: 100%;
  line-height: 15px;
  font-size: 15px;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: rgba(51,51,51,1);
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap; 
}
.music-singerName{
  margin-top: 9px;
  width: 100%;
  line-height: 13px;
  font-size: 13px;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(102,102,102,1);
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap; 
}
.music-slider{
  margin: 0px 23px;
  margin-top: -9px;
}
.music-slider-time{
  margin: 0px 23px;
  padding-bottom: 7px;
  margin-top: 6px;
  line-height: 13px;
  font-size: 12px;
  color: rgba(188,188,188,1);
}
.swiper-music-view{
  width: 100%;
  height: 182px;
}
.music-list-view{
  margin-top: 13px;
  width: 100%;
}
.music-item-view{
  margin: 0px 23px; 
  height: 19px;
  display: flex;
}
.music-playImg{
  margin: 2px 0px;
  width: 15px;
  height: 15px;
}
.music-item-num{
  width: 19px;
  line-height: 19px;
  font-size: 12px;
  font-family: SFProDisplay-Regular;
  font-weight: 400;
  color: rgba(188,188,188,1);
}
.music-item-songName{
  margin-left: 5px;
  width: 100px;
  line-height: 19px;
  font-size: 14px;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: rgba(51,51,51,1);
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap; 
}
.music-item-songNameSele{
  margin-left: 5px;
  width: 100px;
  line-height: 19px;
  font-size: 14px;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: rgba(87,189,106,1);
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.music-item-singerName{
  margin-left: 20px;
  width: calc(100% - 160px);
  line-height: 19px;
  font-size: 13px;
  font-family: PingFangSC-Light;
  font-weight: 300;
  color: rgba(102,102,102,1);
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap; 
}
.music-item-playBtn{
  margin-right: 0px;
  width: 19px;
  height: 19px;
}
.music-item-playBtnImg{
  width: 19px;
  height: 19px;
}